{
    "name": "drnkwati/repack",
    "description": "Illuminate Packages With Minimal Dependencies.",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Nkwati Divine",
            "email": "drnkwati@gmail.com",
            "homepage": "http://dnkwati.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "nesbot/carbon": "~1.17.0",
        "league/flysystem": "~1.0",
        "symfony/finder": "2.7.*",
        "symfony/var-dumper": "2.7.*",
        "symfony/security-core": "2.7.*",

        "illuminate/bus": "5.0.*",
        "illuminate/config": "5.0.*",
        "illuminate/container": "5.0.*",
        "illuminate/contracts": "5.0.*",
        "illuminate/events": "5.0.*",
        "illuminate/hashing": "5.0.*",
        "illuminate/log": "5.0.*",
        "illuminate/mail": "5.0.*",
        "illuminate/pipeline": "5.0.*",
        "illuminate/support": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\": "src/Illuminate/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "suggest": {
        "doctrine/dbal": "Allow renaming columns and dropping SQLite columns."
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
